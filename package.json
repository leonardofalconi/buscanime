{
  "name": "buscanime",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.11.8",
    "graphql": "^16.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "^5.0.1",
    "styled-components": "^6.1.13"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test a --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "server": "npx serve build -p 3000 &",
    "cypress:cli": "yarn build && yarn server",
    "cypress": "yarn cypress:cli && cypress run",
    "cypress:open": "yarn start && cypress open --e2e",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:run": "npx serve storybook-static -p 6006",
    "prepare": "husky",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "eslint src/ --max-warnings 0 && tsc --noemit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@chromatic-com/storybook": "^1.9.0",
    "@eslint/js": "^9.13.0",
    "@storybook/addon-essentials": "^8.3.6",
    "@storybook/addon-interactions": "^8.3.6",
    "@storybook/addon-links": "^8.3.6",
    "@storybook/addon-onboarding": "^8.3.6",
    "@storybook/blocks": "^8.3.6",
    "@storybook/preset-create-react-app": "^8.3.6",
    "@storybook/react": "^8.3.6",
    "@storybook/react-webpack5": "^8.3.6",
    "@storybook/test": "^8.3.6",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^15.0.6",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/cypress": "^1.1.6",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.115",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/styled-components": "^5.1.34",
    "cypress": "^13.15.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-storybook": "^0.10.1",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "prettier": "^3.3.3",
    "prop-types": "^15.8.1",
    "storybook": "^8.3.6",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.11.0",
    "webpack": "^5.95.0"
  }
